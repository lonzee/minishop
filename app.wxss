page {
    background: #fff;
    font-family: '宋体';
}

.scan {
    position: absolute;
}

.scan button {
    border: none;
}

.scanQR {
    height: 30px;
    width: 30px;
    margin-top: 9px;
}

.search {
    padding: 15rpx;
    background: #eee;
}

.webkit_float {
    display: flex;
}

.search input {
    margin-left: 48px;
    width: 300%;
    border-radius: 10px;
    background: #fff;
    border: none;
    font-size: 14px;
    padding: 2%;
    text-align: center;
}

.search button {
    background: none;
    text-align: center;
    border: none;
    padding: 3px;
}

.search button::after {
    content: none;
}

.box_mod .position_relative {
    width: 46%;
    margin-bottom: 10rpx;
    margin-left: 18rpx;
    border-bottom: 1px solid #ededed;
    display: inline-block;
}

.position_relative .photo {
    width: 345rpx;
    height: 345rpx;
}

.box_mod {
    font-weight: bold;
}

.box_mod .title {
    font-size: 28rpx;
    color: #333;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.red {
    color: #dd2727;
}

.priceBlusBox {
    font-size: 26rpx;
    margin-right: 10rpx;
    text-decoration: line-through;
    color: #999;
}

.search_pic {
    width: 10px;
    height: 10px;
}

.properties {
    display: flex;
    color: #999;
}

.properties .title {
    font-size: 28rpx;
}

.saleCount {
    text-align: right;
    width: 50%;
    font-size: 21rpx;
    margin-right: 10rpx;
}

.common_head {
    font-weight: bold;
    border-bottom: 5px solid #ededed;
    font-size: 16px;
    border-radius: 20px;
    padding-left: 15px;
    padding-bottom: 20px;
    padding-top: 15px;
}

.pad_no {
    padding: 10px;
    border-bottom: 3px solid #f3f3f3;
    border-radius: 40rpx;
}

.preview_img {
    width: 265rpx;
    height: 160rpx;
    overflow: hidden;
    border-radius: 5px;
    margin-right: 4%;
    float: left;
}

.overflow_width100 {
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    font-size: 12px;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.price_leading {
    width: 100%;
    overflow: hidden;
    font-size: 12px;
    color: #999;
    padding: 5px;
}

.price_tip {
    width: 100%;
    overflow: hidden;
    font-size: 14px;
    color: #fc0628;
}

.prin {
    border-radius: 5px;
    margin-top: 15px;
    height: 30px;
    width: 75px;
    display: flex;
    align-items: center;
    vertical-align: center;
    justify-content: center;
    flex-direction: row;
}

.gray {
    border: 1px solid #bbbbbb;
    color: #5e5e5e;
}

.font_12 {
    font-size: 12px;
}

.floatright {
    float: right;
}

.marginright {
    margin-right: 5px;
}

.ellipsis {
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    display: block;
}

.colors999 {
    color: #999;
}

.more {
    margin: 10px auto;
    text-align: center;
    height: 70rpx;
    line-height: 70rpx;
    width: 450rpx;
    border-radius: 15rpx;
    color: #fff;
    font-size: 30rpx;
    margin-top: 30rpx;
    background: #e82050;
}

.backgrounds_greenhold {
    background: #62a160;
}

.backgrounds_orange {
    background: #f85;
}

.backgrounds_red {
    background: #dd2727;
}

.white {
    color: #fff;
}

.floatleft {
    float: left;
}

.position_serial {
    padding: 3%;
    border-bottom: 10px solid #f3f3f3;
    border-radius: 20px;
}

.font_14 {
    font-size: 14px;
}

.font_20 {
    font-size: 20px;
    font-weight: bold;
}

.margin_top5 {
    margin-top: 5px;
}

.colors33 {
    color: #333;
}

.textcenter {
    text-align: center;
}

.width_100 {
    width: 100%;
}

.depers {
    flex: 1;
}