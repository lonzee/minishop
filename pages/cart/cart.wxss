.container {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
    box-sizing: border-box;
    height: 100%;
    padding: 200rpx 0;
    font-family: '宋体';
}

.carts-list {
    display: flex;
    flex-direction: column;
    padding: 20rpx 40rpx;
    background: #fff;
}

.carts-item {
    display: flex;
    flex-direction: column;
    height: 150rpx;
    width: 100%;
    border: 20rpx solid #f3f3f3;
    padding: 30rpx 0;
    border-radius: 40rpx;
}